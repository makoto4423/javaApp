aweb_user:
  `id` varchar(50) NOT NULL DEFAULT '' COMMENT '用户ID',
  `create_time` varchar(32) DEFAULT NULL COMMENT '创建时间',
  `create_user_id` varchar(32) DEFAULT NULL COMMENT '创建用户ID',
  `create_user_name` varchar(32) DEFAULT NULL COMMENT '创建用户名称',
  `name` varchar(32) DEFAULT NULL COMMENT '用户名',
  `update_time` varchar(32) DEFAULT NULL COMMENT '更新时间',
  `update_user_id` varchar(32) DEFAULT NULL COMMENT '更新用户ID',
  `update_user_name` varchar(32) DEFAULT NULL COMMENT '更新用户名',
  `email` varchar(100) DEFAULT NULL COMMENT '邮箱',
  `nickname` varchar(32) DEFAULT NULL COMMENT '昵称',
  `password` varchar(255) DEFAULT NULL COMMENT '密码',
  `phone` varchar(18) DEFAULT NULL COMMENT '电话号码',
  `status` varchar(1) DEFAULT NULL COMMENT '用户状态0-新用户,1-正常，2-锁定',
  `description` varchar(255) DEFAULT NULL COMMENT '描述',
  `type` varchar(1) DEFAULT NULL COMMENT '用户类型0-超级管理员，1-普通用户',
  `last_host_address` varchar(32) DEFAULT NULL COMMENT '最后登录地址',
  `last_login_time` varchar(32) DEFAULT NULL COMMENT '最后登录时间',
  `failed_login_count` varchar(2) DEFAULT NULL COMMENT '连续登录错误数',
  `last_failed_login_time` varchar(32) DEFAULT NULL COMMENT '上次登录失败时间',
  `lock_time` varchar(32) DEFAULT NULL COMMENT '用户锁定时间',
  `USR_PASSWORD_UPDATETIME` varchar(30) DEFAULT NULL COMMENT '密码修改时间',
  `USR_REMARK` varchar(200) DEFAULT NULL COMMENT '备注',
  `USR_PASSWORD` varchar(300) DEFAULT NULL COMMENT 'afa密码',
  `IS_DELETE` int(1) DEFAULT '0' COMMENT '是否删除：0否 1是',
  `SETTING` varchar(50) DEFAULT NULL,
  `ORGANIZATION_ID` varchar(50) DEFAULT NULL,
  `JOB` varchar(50) DEFAULT NULL,
  `password_text` varchar(50) DEFAULT NULL,
  `secret_key` varchar(50) DEFAULT NULL,

aweb_authority:
  `id` varchar(50) NOT NULL,
  `create_time` datetime DEFAULT NULL,
  `create_user_id` varchar(255) DEFAULT NULL,
  `create_user_name` varchar(255) DEFAULT NULL,
  `name` varchar(255) DEFAULT NULL,
  `update_time` datetime DEFAULT NULL,
  `update_user_id` varchar(255) DEFAULT NULL,
  `update_user_name` varchar(255) DEFAULT NULL,
  `status` varchar(2) DEFAULT NULL,
  `parent_id` varchar(50) DEFAULT NULL,
  `type` varchar(50) DEFAULT NULL,

aweb_menu:
  `id` varchar(50) NOT NULL COMMENT '整合AFA5管控时，id为原AFA5菜单表的MEN_ID字段值',
  `create_time` varchar(255) DEFAULT NULL,
  `create_user_id` varchar(255) DEFAULT NULL,
  `create_user_name` varchar(255) DEFAULT NULL,
  `name` varchar(255) DEFAULT NULL,
  `update_time` varchar(255) DEFAULT NULL,
  `update_user_id` varchar(255) DEFAULT NULL,
  `update_user_name` varchar(255) DEFAULT NULL,
  `icon` varchar(50) DEFAULT NULL,
  `parent_id` varchar(50) DEFAULT NULL,
  `path` varchar(400) DEFAULT NULL COMMENT '整合AFA5管控时，path需以‘iframe’开头',
  `seq` varchar(5) DEFAULT NULL,
  `status` varchar(255) DEFAULT NULL,
  `title` varchar(50) DEFAULT NULL,
  `url` varchar(50) DEFAULT NULL COMMENT '仅整合AFA5管控时使用，url为原AFA5菜单表的MEN_VALUE字段值',
  `type` varchar(50) DEFAULT NULL,

aweb_role:
  `id` varchar(50) NOT NULL,
  `create_time` varchar(32) DEFAULT NULL,
  `create_user_id` varchar(32) DEFAULT NULL,
  `create_user_name` varchar(32) DEFAULT NULL,
  `name` varchar(32) DEFAULT NULL,
  `update_time` varchar(32) DEFAULT NULL,
  `update_user_id` varchar(32) DEFAULT NULL,
  `update_user_name` varchar(32) DEFAULT NULL,
  `description` varchar(200) DEFAULT NULL,
  `tenant_id` varchar(50) DEFAULT NULL,
  `status` varchar(50) DEFAULT NULL,

aweb_tenant:
  `ID` varchar(50) NOT NULL DEFAULT '' COMMENT '租户ID',
  `NAME` varchar(50) NOT NULL DEFAULT '' COMMENT '租户名称',
  `DESCRIPTION` varchar(200) DEFAULT NULL COMMENT '租户描述',
  `CREATE_TIME` varchar(50) DEFAULT NULL COMMENT '租户创建时间',
  `CREATE_USER_ID` varchar(50) DEFAULT NULL COMMENT '租户创建用户ID',
  `CREATE_USER_NAME` varchar(50) DEFAULT NULL COMMENT '租户创建用户名',
  `UPDATE_TIME` varchar(50) DEFAULT NULL COMMENT '租户更新时间',
  `UPDATE_USER_ID` varchar(50) DEFAULT NULL COMMENT '租户更新用户ID',
  `UPDATE_USER_NAME` varchar(50) DEFAULT NULL COMMENT '租户更新用户名',
  `cust_pro_code` varchar(255) DEFAULT NULL,
  `owner_id` varchar(255) DEFAULT NULL,
  `owner_name` varchar(255) DEFAULT NULL,
  `owner_nickname` varchar(255) DEFAULT NULL,
  `setting` varchar(255) DEFAULT NULL,
  `type` varchar(255) DEFAULT NULL,

aweb_environment:
  `ID` varchar(50) NOT NULL COMMENT 'ID',
  `NAME` varchar(200) DEFAULT NULL COMMENT '名称',
  `DESCRIPTION` varchar(300) DEFAULT NULL COMMENT '描述',
  `CREATE_TIME` varchar(25) DEFAULT NULL COMMENT '创建时间',
  `CREATE_USER_NAME` varchar(50) DEFAULT NULL COMMENT '创建用户NAME',
  `CREATE_USER_ID` varchar(50) DEFAULT NULL COMMENT '创建用户ID',
  `UPDATE_TIME` varchar(30) DEFAULT NULL COMMENT '更新时间',
  `UPDATE_USER_ID` varchar(50) DEFAULT NULL COMMENT '更新用户ID',
  `UPDATE_USER_NAME` varchar(30) DEFAULT NULL COMMENT '更新用户NAME',